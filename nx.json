{
    "namedInputs": {
        "env": [
            {
              "runtime": "node -v"
            },
            {
              "runtime": "echo 'CACHE-BUST-1'"
            }
        ],
        "build": [
        "{projectRoot}/src/**/*",
        "!{projectRoot}/**/*.test.ts",
        "!{projectRoot}/**/*.stories.tsx",
        "!{projectRoot}/**/__fixtures__/**/*",
        "!{projectRoot}/**/__tests__/**/*",
        "!{projectRoot}/**/*.md"
        ],
        "test": [
        "{projectRoot}/src/**/*",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/.browserslistrc",
        "{projectRoot}/jest.config.js",
        "!{projectRoot}/**/*.stories.tsx",
        "!{projectRoot}/**/*.md"
        ]
    },
    "targetDefaults": {
        "prepare": {
            "inputs": ["env", "build", "^build"]
        }
    }
}